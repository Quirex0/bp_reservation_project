// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Place {
  Praha_8_Bohnice
  Praha_8_Karlin
  Plzen
}

model Reservation {
  id String @default(cuid())
  @@id([date, place])
  place Place
  firstName String
  lastName String
  email String
  date DateTime
}
